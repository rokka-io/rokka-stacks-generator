{
    "name": "@ayalon/rokka-stacks-generator",
    "version": "1.0.2",
    "description": "Generate and upload rokka stacks based on yml files in a folder.",
    "main": "./dist/index.js",
    "bin": {
        "rokka-stacks-generator": "./dist/index.js"
    },
    "license": "MIT",
    "private": false,
    "scripts": {
        "watch": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
        "start": "ts-node -r dotenv/config src/index.ts -f ../test -o ../test/output/styles.json",
        "build": "tsc -p .",
        "lint": "tsc --noEmit && eslint . --ext .js,.jsx,.ts,.tsx",
        "pretty": "prettier --write '**/*.{js,ts}'"
    },
    "devDependencies": {
        "@types/glob": "^7.1.4",
        "@types/yaml": "^1.9.7",
        "@typescript-eslint/eslint-plugin": "^4.32.0",
        "@typescript-eslint/parser": "^4.32.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-import": "2.24.2",
        "nodemon": "2.0.13",
        "prettier": "2.4.1",
        "pretty-quick": "3.1.1",
        "ts-node": "10.2.1",
        "typescript": "^4.4.3"
    },
    "dependencies": {
        "@types/node": "16.10.2",
        "commander": "8.2.0",
        "dotenv": "^10.0.0",
        "glob": "^7.2.0",
        "rokka": "^3.0.1",
        "yaml": "^1.10.2"
    }
}
